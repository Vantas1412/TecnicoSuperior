# Rename this file to `.env` and fill in your own values before running the project.

# Supabase credentials
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Email service configuration
# URL to your email backend.  For the Node.js backend included in this project,
# use http://localhost:3001/send-email (or your deployed URL if running
# elsewhere).
VITE_EMAIL_API_URL=http://localhost:3001/send-email

# API key to include in the Authorization header when calling the email backend.
# If you set EMAIL_API_KEY in backend/.env, this value must match.  Leave
# empty to disable API key checks.
VITE_EMAIL_API_KEY=

# Default sender address used in email notifications.  Usually the same as
# your Gmail username.  If left blank, the backend will use its configured
# GMAIL_USER.
VITE_EMAIL_FROM=

# Libelula Payment Gateway
VITE_LIBELULA_APPKEY=
VITE_LIBELULA_API_URL=https://sandbox.libelula.bo/api/v1
VITE_LIBELULA_SANDBOX=true

# Google reCAPTCHA v2
# Get your keys at: https://www.google.com/recaptcha/admin
VITE_RECAPTCHA_SITE_KEY=